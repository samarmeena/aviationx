{
  "name": "aviationx",
  "version": "3.0.0",
  "private": true,
  "license": "Apache-2.0",
  "type": "module",
  "main": "build/main.js",
  "scripts": {
    "build": "tsc",
    "build:changelog": "npx @discordx/changelog --root=src",
    "dev": "ts-node-esm src/main.ts",
